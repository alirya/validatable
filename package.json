{
  "name": "@dikac/t-validatable",
  "version": "0.13.0",
  "description": "Typescript validatable",
  "dependencies": {
    "@dikac/deployer": "^0.1.5",
    "@dikac/t-boolean": "^0.6.1",
    "@dikac/t-function": "^0.4.2",
    "@dikac/t-iterable": "^0.1.1",
    "@dikac/t-message": "^0.5.3",
    "@dikac/t-object": "^0.17.3",
    "@dikac/t-value": "^0.8.1",
    "ts-toolbelt": "^6.12.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.11",
    "cpy-cli": "^3.1.1",
    "jasmine": "^3.4.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "json-properties-remover": "^0.1.2",
    "move-cli": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  },
  "scripts": {
    "spec-run": "jasmine --reporter=jasmine-ts-console-reporter --config=./jasmine.json",
    "spec": "npm-run-all spec-compile spec-run",
    "spec-compile": "npm-run-all --parallel spec-compile-script ",
    "spec-compile-watch": "npm-run-all --parallel spec-compile-watch-script",
    "spec-compile-script": "tsc -p tsconfig-spec.json",
    "spec-compile-watch-script": "tsc -w -p tsconfig-spec.json",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "release": "remove-json-properties dist/package.json private remove git install script on npm install",
    "postrelease": "npm publish dist",
    "prerelease": "cpy     package.json package-lock.json LICENSE Readme.md .npmignore dist",
    "postpublish": "rimraf package.json package-lock.json LICENSE Readme.md .npmignore",
    "postinstall": "npm run deploy",
    "deploy": "deployer /.git /dist /"
  },
  "private": true,
  "author": "",
  "license": "MIT"
}
